{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "SpatialLite.*/*.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "SpatialLITE",
      "_appLogoPath": "images/icon.png",
      "_appFooter": "(c)2017 Lukas Kabrt, generated by DocFx",
      "_enableSearch": "true",
      "_gitContribute": {
        "repo ": "https://github.com/lukaskabrt/SpatialLITE",
        "branch": "master",
        "apiSpecFolder": "docs/apidoc"
      }
    },
    "fileMetadataFiles": [],
    "template": [
      "default"
    ]
  }
}